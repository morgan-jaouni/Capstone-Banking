			  <label for="name">Name: </label>
			  <input  id="name" name="name" type="text" value="<%= userdata.name %>"></input>
			</div>
	  
			<div>
			  <label for="userName">Username: </label>
			  <input  id="userName" name="username" type="text" value="<%= userdata.username %>"></input>
			</div>
	  
			<div>
			  <label for="password">Password: </label>
			  <input  id="password" name="password" type="text" value="<%= userdata.password %>"></input>
			</div>
			<div>
			  <label for="password">Email: </label>
			  <input  id="password" name="email" type="text" value="<%= userdata.email %>"></input>
			</div>
			<button  type="submit">Update</button>
		  </form>
